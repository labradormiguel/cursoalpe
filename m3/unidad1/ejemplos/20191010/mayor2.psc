Algoritmo mayor2
	// Introduzco dos números y me devuelve el mayor
	num1 <- 0
	num2 <- 0
	salida <- 0
	Escribir 'Escribe el primer número'
	Leer num1
	Escribir 'Escribe el segundo número'
	Leer num2
	Si num1>num2 Entonces
		salida <- num1
	SiNo
		salida <- num2
	FinSi
	Escribir "El número mayor es " salida
FinAlgoritmo

